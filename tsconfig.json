{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "esnext",
      "dom"
    ],
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": true,
    "module": "esnext",
    "types": [
      "node",
      "@wdio/globals/types",
      "@wdio/mocha-framework",
      "mocha",
      "expect-webdriverio"
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}